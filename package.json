{
  "name": "sample-app",
  "description": "",
  "author": "",
  "version": "0.1.0",
  "homepage": "",
  "license": "",
  "main": "app.js",
  "readmeFilename": "",
  "engines": {
    "node": ">=0.10.0"
  },
  "engineStrict": true,
  "dependencies": {
    "body-parser": "^1.14.2",
    "roosevelt": "0.7.4",
    "roosevelt-closure": "0.3.0",
    "roosevelt-less": "0.2.6",
    "teddy": "0.4.4"
  },
  "devDependencies": {
    "jshint": "*",
    "supervisor": "0.12.0",
    "reload": "1.1.1"
  },
  "rooseveltConfig": {
    "port": 43711,
    "localhostOnly": true,
    "disableLogger": false,
    "noMinify": false,
    "multipart": {
      "multiples": true
    },
    "maxLagPerRequest": 2000,
    "shutdownTimeout": 30000,
    "modelsPath": "mvc/models",
    "modelsNodeModulesSymlink": "models",
    "viewsPath": "mvc/views",
    "viewEngine": "html: teddy",
    "controllersPath": "mvc/controllers",
    "error404": "404.js",
    "error5xx": "5xx.js",
    "error503": "503.js",
    "staticsRoot": "statics",
    "cssPath": "css",
    "cssCompiler": {
      "nodeModule": "roosevelt-less",
      "params": {
        "compress": true
      }
    },
    "cssCompilerWhitelist": null,
    "cssCompiledOutput": ".build/css",
    "jsPath": "js",
    "jsCompiler": {
      "nodeModule": "roosevelt-closure",
      "params": {
        "compilation_level": "ADVANCED_OPTIMIZATIONS"
      }
    },
    "jsCompilerWhitelist": null,
    "jsCompiledOutput": ".build/js",
    "publicFolder": "public",
    "symlinksToStatics": [
      "css: .build/css",
      "images",
      "js: .build/js"
    ],
    "versionedStatics": false,
    "versionedCssFile": null,
    "alwaysHostPublic": false
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "devel": true,
    "indent": 2,
    "node": true
  },
  "private": true,
  "repository": {
    "private-repo": "git+ssh://somewhere:port/folder/on/server"
  },
  "scripts": {
    "start": "npm run prod",
    "prod": "supervisor -k -e js,json,less,html -i statics/.build/,public/,.git -- app.js -prod",
    "dev": "supervisor -k -e js,json,less,html -i statics/.build/,public/,.git -RV -- app.js -dev",
    "test": "jshint app.js"
  }
}
